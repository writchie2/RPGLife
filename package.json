    "font-setup": "expo install expo-font @expo-google-fonts/metamorphous @expo-google-fonts/alegreya",
    "setup": "npm run expo-setup && npm run datePicker-setup && npm run font-setup"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/alegreya": "^0.2.3",
    "@expo-google-fonts/metamorphous": "^0.2.3",
